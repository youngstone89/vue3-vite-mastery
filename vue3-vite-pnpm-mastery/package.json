    "axios": "^1.13.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "express": "^4.21.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "highcharts": "^11.4.8",
    "highcharts-vue": "^2.0.1",
    "nanoid": "^5.0.7",
    "node-fetch": "^3.3.2",
    "pinia": "^2.2.2",
    "v-calendar": "^3.1.2",
    "vite-plugin-glob": "^0.3.2",
    "vue": "^3.5.8",
    "vue-router": "^4.4.5"
